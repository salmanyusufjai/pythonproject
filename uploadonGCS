pip install google-cloud-storage

from google.cloud import storage

def upload_file_to_gcs(bucket_name, source_file_name, destination_blob_name):
    """Uploads a file to the GCS bucket."""
    
    # Create a client
    storage_client = storage.Client()
    
    # Get the bucket
    bucket = storage_client.bucket(bucket_name)
    
    # Create a new blob (file object in the bucket)
    blob = bucket.blob(destination_blob_name)
    
    # Upload the file to the blob
    blob.upload_from_filename(source_file_name)
    
    print(f"File {source_file_name} uploaded to {destination_blob_name} in bucket {bucket_name}.")

# Usage example:
bucket_name = "your-gcs-bucket-name"
source_file_name = "path/to/your/auditfile.txt"  # Local file you want to upload
destination_blob_name = "folder/in/bucket/auditfile.txt"  # Path in GCS bucket

upload_file_to_gcs(bucket_name, source_file_name, destination_blob_name)
